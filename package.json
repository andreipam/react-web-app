{
  "name": "react-large-project-example-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "material-ui": "^1.0.0-beta.24",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
